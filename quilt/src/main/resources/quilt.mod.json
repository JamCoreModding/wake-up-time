{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.jamalam360",
    "id": "wake_up_time",
    "version": "${version}",
    "metadata": {
      "name": "Wake Up Time",
      "description": "Figure out what stage of the day your villagers are in",
      "contributors": {
        "Jamalam": "Owner"
      },
      "contact": {
        "sources": "https://github.com/JamCoreModding/wake-up-time"
      },
      "icon": "assets/wake_up_time/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "client_init": ["io.github.jamalam360.wake_up_time.quilt.WakeUpTimeQuilt"]
