    "depends": [
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": ">=${architectury_version}"
      },
      {
